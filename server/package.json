{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "^8.2.2",
    "@types/mongodb": "^3.6.12",
    "@types/node": "^14.14.41",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "concurrently": "^6.0.2",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "start": "yarn run build && node build/index.js",
    "start:dev": "nodemon",
    "test": "env mocha -r ts-node/register 'src/tests/**/*.ts'",
    "client": "cd ../client && yarn start",
    "setup": "./setup.sh",
    "dev": "concurrently \"yarn run start:dev\" \"yarn run client\"",
    "client:typedoc": "cd ../client && yarn typedoc --watch --readme ../README.md --out ../docs/client src/",
    "server:typedoc": "yarn typedoc --watch --readme ../README.md --out ../docs/server src/",
    "dev:typedoc": "concurrently \"yarn run client:typedoc\" \"yarn run server:typedoc\""
  },
  "dependencies": {
    "mongodb": "^3.6.6"
  }
}
